{
  "name": "velzon-cta",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "cta",
  "sourceRoot": "apps/velzon-cta/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/velzon-cta",
        "index": "apps/velzon-cta/src/index.html",
        "main": "apps/velzon-cta/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/velzon-cta/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "libs/velzon-sdk/src/assets",
            "output": "assets"
          }
        ],
         "stylePreprocessorOptions": {
          "includePaths": ["libs/velzon-sdk/src/assets"]
        },
        "styles": [
           "apps/velzon-cta/src/styles.scss",
           "libs/velzon-sdk/src/assets/scss/config/corporate/bootstrap.scss",
           "libs/velzon-sdk/src/assets/scss/config/corporate/app.scss",
           "libs/velzon-sdk/src/assets/scss/icons.scss"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/velzon-cta/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/velzon-cta/webpack.prod.config.ts"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "port": 4201,
        "publicHost": "http://localhost:4201"
      },
      "configurations": {
        "production": {
          "buildTarget": "velzon-cta:build:production"
        },
        "development": {
          "buildTarget": "velzon-cta:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "velzon-cta:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/velzon-cta/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "velzon-cta:build",
        "port": 4201,
        "watch": true
      },
      "configurations": {
        "development": {
          "buildTarget": "velzon-cta:build:development"
        },
        "production": {
          "buildTarget": "velzon-cta:build:production"
        }
      }
    }
  }
}
